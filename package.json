{
  "name": "multi-tenant-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "test": "node ./node_modules/mocha/bin/mocha",
    "test-apis": "node ./node_modules/mocha/bin/mocha ./test/api/",
    "test-routes": "node ./bin/www & P1=$! && sleep 2 && node ./node_modules/mocha/bin/mocha ./test/routes/v1/ && kill $P1",
    "test-routes-win": "start cmd /c node ./bin/www && ping -n 3 127.0.0.1 > nul && node ./node_modules/mocha/bin/mocha ./test/routes/v1/ && taskkill /F /IM node.exe",
    "test-eslint": "eslint app.js bin/www routes/*.js api/*.js"
  },
  "pre-commit": [
    "test-eslint"
  ],
  "dependencies": {
    "@mailchimp/mailchimp_transactional": "^1.0.50",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "generate-password": "^1.7.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uniqid": "^5.4.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "chai": "^4.3.9",
    "chai-http": "^4.4.0",
    "cypress": "^13.3.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2"
  }
}
